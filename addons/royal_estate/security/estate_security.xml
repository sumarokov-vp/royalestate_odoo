<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="module_category_estate" model="ir.module.category">
        <field name="name">Royal Estate</field>
        <field name="sequence">10</field>
    </record>

    <record id="group_estate_user" model="res.groups">
        <field name="name">Estate User</field>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
    </record>

    <record id="group_estate_agent" model="res.groups">
        <field name="name">Estate Agent</field>
        <field name="implied_ids" eval="[(4, ref('group_estate_user'))]"/>
    </record>

    <record id="group_estate_coordinator" model="res.groups">
        <field name="name">Estate Coordinator</field>
        <field name="implied_ids" eval="[(4, ref('group_estate_agent'))]"/>
    </record>

    <record id="group_estate_manager" model="res.groups">
        <field name="name">Estate Manager</field>
        <field name="implied_ids" eval="[(4, ref('group_estate_coordinator'))]"/>
    </record>
</odoo>
